    "import turtle\n",
    "t = turtle.Pen()\n",
    "\n",
    "for _ in range (6):\n",
    "    t.forward(100)\n",
    "    t.left(90)\n",
    "t.right(60)\n",
    "t.forward(100)\n",
    "t.left(120)\n",
    "t.forward(100)\n",
    "t.penup()\n",
    "t.forward(200)\n",
    "t.write(\"Name\", font=(\"Arial\", 18, \"normal\"))\n",
    "\n",
    "turtle.done()"
   ]
